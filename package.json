{
  "name": "@v3b/project-manager",
  "version": "0.2.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "vpm": "./bin/cli.js"
  },
  "engines": {
    "node": ">=20"
  },
  "files": ["bin", "dist"],
  "scripts": {
    "build": "rslib build",
    "check": "biome check --write",
    "dev": "rslib build --watch",
    "format": "biome format --write",
    "test": "vitest run",
    "prepublishOnly": "rm -rf dist && pnpm run build"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "chalk": "^5.3.0",
    "clipboardy": "^4.0.0",
    "commander": "^11.1.0",
    "inquirer": "^12.6.3",
    "simple-git": "^3.22.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.3",
    "@rslib/core": "^0.1.4",
    "@types/inquirer": "^9.0.8",
    "@types/node": "^22.10.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.8"
  },
  "packageManager": "pnpm@9.15.0"
}
